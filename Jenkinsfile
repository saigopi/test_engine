pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building_Test_Engine..'
            }
        }
        stage('Test') {
            steps {
                echo 'Build Completed - Testing_Test_Engine..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Test Completed - Deploying_Test_Engine....'
            }
        } catch (error) {
            throw error
    } finally {
}
}
